{
  "name": "nteract-on-jupyter",
  "version": "1.2.0",
  "description": "nteract on jupyter, as an extension",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "dev": "echo 'run a notebook server with this' && npm run build:watch",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Kyle Kelley <rgbkrk@gmail.com>",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@nteract/commutable": "^3.1.0",
    "@nteract/core": "^2.0.0",
    "@nteract/notebook-preview": "^5.1.0",
    "@nteract/transform-dataresource": "^3.0.0",
    "@nteract/transform-vega": "^3.1.0",
    "@nteract/transforms": "^3.2.0",
    "@nteract/types": "^1.1.0",
    "jquery": "^3.2.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-notification-system": "^0.2.16",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-observable": "^0.17.0",
    "rx-jupyter": "^2.3.5",
    "rxjs": "^5.5.6"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      "transform-flow-strip-types"
    ]
  },
  "devDependencies": {
    "next": "^4.2.1",
    "styled-jsx": "^2.2.1"
  }
}
