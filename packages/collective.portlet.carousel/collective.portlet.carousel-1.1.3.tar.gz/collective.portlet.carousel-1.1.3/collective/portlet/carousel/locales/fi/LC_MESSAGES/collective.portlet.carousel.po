#: collective/portlet/carousel/browser.py:114
msgid "Add carousel portlet"
msgstr "Lisää karusellisovelma"

#: collective/portlet/carousel/interfaces.py:125
msgid "Automatic rotation"
msgstr "Pyöritä automaattisesti"

#: collective/portlet/carousel/interfaces.py:49
msgid "Carousel background"
msgstr "Karusellin ensisijainen kuva"

#: collective/portlet/carousel/interfaces.py:55
msgid "Carousel background link"
msgstr "Karusellin toissijainen kuva"

#: collective/portlet/carousel/interfaces.py:78
msgid "Carousel caption"
msgstr "Karusellin kuvateksti"

#: collective/portlet/carousel/interfaces.py:42
msgid "Carousel description"
msgstr "Karusellin kuvaus"

#: collective/portlet/carousel/interfaces.py:71
msgid "Carousel external link"
msgstr "Karusellin ulkoinen linkki"

#: collective/portlet/carousel/interfaces.py:35
msgid "Carousel heading"
msgstr "Karusellin otsikko"

#: collective/portlet/carousel/interfaces.py:63
msgid "Carousel link"
msgstr "Karusellin sisäinen linkki"

#: collective/portlet/carousel/interfaces.py:92
msgid "Collection reference"
msgstr "Kokoelma"

#: collective/portlet/carousel/browser.py:122
msgid "Edit carousel portlet"
msgstr "Muokkaa karusellisovelmaa"

#: collective/portlet/carousel/interfaces.py:72
msgid "Entering a manual link here will override  any selection in the link field above."
msgstr "Tähän syötetty URL toimii suoraan karusellin linkkinä (jättäen sisäisen linkin huomiotta)"

#: collective/portlet/carousel/interfaces.py:120
msgid "How long to display each banner (in seconds)."
msgstr "Kuinka kauan jokaista sisältöä näytetään (sekuntaina)"

#: collective/portlet/carousel/interfaces.py:102
msgid "If no collection is selected the following elements will be displayed in the carousel"
msgstr "Kun kokoelmaa ei ole valittuna, näytetään karusellissa tähän valittu sisältö (valitun sisällön täytyy kuitenkin tukea karusellia)"

#: collective/portlet/carousel/interfaces.py:56
msgid "If selected this link will be used from background in the carousel"
msgstr "Tämä kuva näytetään karusellissa, jos ensisijaista kuvaa ei ole valittuna"

#: collective/portlet/carousel/interfaces.py:64
msgid "If selected this link will be used from the carousel item, otherwise a link to this object is used"
msgstr "Jätä tämä tyhjäksi, jos haluat karusellin linkittävän tähän sisältöön"

#: collective/portlet/carousel/interfaces.py:43
msgid "If this description is not specified the description of the object will be used as description"
msgstr "Jätä tämä tyhjäksi, jos haluat näyttää karusellissä sisällön oman kuvauksen"

#: collective/portlet/carousel/interfaces.py:36
msgid "If this heading is not specified the title of the object will be used as heading"
msgstr "Jätä tämä tyhjäksi, jos haluat näyttää karusellissä sisällön oman otsikon"

#: collective/portlet/carousel/interfaces.py:114
msgid "Number of elements to be shown in the carousel"
msgstr "Sisällön enimmäismäärä (kpl)"

#: collective/portlet/carousel/interfaces.py:131
msgid "Omit portlet border"
msgstr "Ei sovelman reunoja"

#: collective/portlet/carousel/interfaces.py:88
msgid "Portlet header"
msgstr "Sovelman otsikko"

#: collective/portlet/carousel/interfaces.py:101
msgid "References to elements"
msgstr "Muu sisältö"

#: collective/portlet/carousel/interfaces.py:119
msgid "Rotation speed"
msgstr "Pyöritysnopeus"

#: collective/portlet/carousel/interfaces.py:93
msgid "Select the collection that should be used to fetch the elements that are shown in the carousel"
msgstr "Valitse kokokelma, jonka löytämä sisältö karusellissa näytetään"

#: collective/portlet/carousel/interfaces.py:50
msgid "This image is used as background in the carousel"
msgstr "Tämä kuva näytetään karusellissa, jos toissijaista kuvaa ei ole valittu"

#: collective/portlet/carousel/interfaces.py:132
msgid "Tick this box if you want to render the text above without the standard header, border or footer"
msgstr "Kun tämä on valittu, karuselli näytetään ilman sovelmien tavanomaista otsikkoa, alaviitettä tai reunaviivoja"

#. Default: "Carousel"
#: collective/portlet/carousel/interfaces.py:22
msgid "fieldset_carousel"
msgstr "Karuselli"
